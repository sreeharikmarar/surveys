<div class="container" style="background-color:#fff" >

<h1><%= @feedback.survey.title %></h1>
<p><b><%= @feedback.survey.description %></b></p>

<table class="table table-striped">
    <thead>
        <tr>
          <th>Questions</th>
          <th>Answers</th>
        </tr>
    </thead>

	<tbody>

		<% @feedback.answers.each do |answer| %>
			<tr>
				<td><%= answer.question.text %></td>
				<td><%= answer.answer %></td>
			</tr>
		<% end %>
	</tbody>
</table>
</div>